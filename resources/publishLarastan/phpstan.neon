includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 8 is the highest level
    level: 6

    ignoreErrors:

    excludePaths:
        - **/*Template*.php
        - **/ExtendBlueprint.php
        - **/helpers.php
        - app/View

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
